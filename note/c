char   c   =   'a';   //一个字符型变量

char   *p;       //p是一个指向char类型的指针

p=&c;             //现在p里存的是c的地址

char   **q;     //q是一个二级指针

q=&p;             //q里存的是p的地址(p如前所述是个整数,所以p占有内存的一块空间,现在q就是指向这个空间的开始)

printf("%p",q);       //p的地址

printf("%p",*q);     //c的地址

printf("%c",**q);   //a

int   A[100];   //A是一个int   *型的指针,指向一块内存的开始(当然课本上把这种指向预先分配的空间的指针叫做数组)

int   B[100][100];     //B就是一个int   **型的指针

void   F(int   **p);